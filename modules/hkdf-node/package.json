  },
  "license": "Apache-2.0",
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "sideEffects": false,
  "main": "./build/main/src/index.js",
  "module": "./build/module/src/index.js",
  "types": "./build/main/src/index.d.ts",
  "files": [
    "build/**/src/*"
  ],
  "standard": {
    "fix": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ]
